{
    "extends": "strongloop",
    "parserOptions": {
        "ecmaVersion": 2020
    },
    "env": {
        "es6": true
    },
    "rules": {
        "max-len": [
            "error",
            100
        ],
        "quotes": [
            "error",
            "double"
        ],
        "space-before-function-paren": "off",
        "indent": [
            "error",
            4,
            {
                "MemberExpression": 0,
                "SwitchCase": 1,
                "CallExpression": {
                    "arguments": "off"
                }
            }
        ]
    }
}